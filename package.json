{
  "private": true,
  "name": "remix-movie",
  "version": "0.0.1",
  "description": "",
  "license": "",
  "scripts": {
    "build": "yarn run prod:css && yarn run build:worker && remix build",
    "build:css": "postcss styles/**/*.css --dir app/styles",
    "prod:css": "yarn run build:css",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev": "concurrently \"yarn dev:css\" \"yarn dev:worker\" \"remix dev\"",
    "dev:css": "yarn run build:css --watch",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "prebuild": "rimraf ./public/build \"./app/styles/**/*.css\"",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "prepare": "husky install"
  },
