{
  "private": true,
  "name": "remix-app-template",
  "version": "0.0.1",
  "description": "",
  "license": "",
  "scripts": {
    "build": "yarn run build:css && remix build",
    "build:css": "tailwindcss -o app/styles/app.css",
    "dev": "concurrently \"yarn run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o app/styles/app.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "check:lint": "eslint . --ext ts --ext tsx --ext js",
    "check:format": "prettier --check .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.2",
    "@mui/material": "^5.8.2",
    "@nextui-org/react": "^1.0.6-beta.5",
    "@remix-run/react": "^1.3.4",
    "@remix-run/serve": "^1.3.4",
    "@remix-run/vercel": "^1.3.4",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "eslint-import-resolver-typescript": "^2.7.1",
    "postcss": "^8.4.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.3.4",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.4",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^7.0.0",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*": [
      "yarn eslint",
      "yarn prettier -c"
    ]
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
